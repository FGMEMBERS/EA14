<?xml version="1.0"?>

<!-- Pipistrel Taurus Electro  -->
<!-- 2016 D-ECHO -->

<PropertyList>

 <path>ea14.ac</path>

 <offsets>
   <x-m>0.</x-m>
   <y-m>0.</y-m>
   <z-m>0.</z-m>
   <pitch-deg>0.0</pitch-deg>
 </offsets>
<nasal>
	<load>
      var livery_update = aircraft.livery_update.new("Aircraft/TaurusElectro/Models/Liveries", 10);
		
      var float_0 = self.getNode("sim/multiplay/generic/float[0]", 1);
      var float_1 = self.getNode("sim/multiplay/generic/float[1]", 1);
      
      var crew_door = self.getNode("sim/model/door-positions/crew/position-norm", 1);
      var engine_door = self.getNode("sim/model/door-positions/engine/position-norm", 1);
      
      crew_door.alias(float_0);
      engine_door.alias(float_1);
	</load>
	<unload>
      livery_update.stop();
      crew_door.unalias();
      engine_door.unalias();
	</unload>
</nasal>
		

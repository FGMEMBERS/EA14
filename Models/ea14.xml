<!--Prop-->

  <animation>
    <type>spin</type>
    <object-name>prop</object-name>
    <object-name>propdisc</object-name>
    <property>engines/engine/rpm</property>
<condition><greater-than>
    <property>sim/model/door-positions/engine/position-norm</property>
<value>0.99</value></greater-than></condition>
    <factor>1</factor>
		<center>
			<x-m>-0.50373</x-m>
			<y-m>0.0</y-m>
			<z-m>1.19814</z-m>
		</center>
		<axis>
			<x>1</x>
			<y>0</y>
			<z>0</z>
		</axis>
  </animation>
  <animation>
    <type>select</type>
    <object-name>prop</object-name>
<condition><less-than>
    <property>engines/engine/rpm</property>
<value>1000</value>
</less-than>
</condition>
  </animation>
  <animation>
    <type>select</type>
    <object-name>propdisc</object-name>
<condition><and>
<greater-than>
    <property>engines/engine/rpm</property>
<value>1000</value>
</greater-than>
<greater-than>
    <property>sim/model/door-positions/engine/position-norm</property>
<value>0.99</value></greater-than></and>
</condition>
  </animation>

<!--Gear-->
<animation>
    <type>rotate</type>
    <object-name>wheelF</object-name>
    <property>controls/flight/rudder</property>
    <factor>-25</factor>
    <axis>
      <x1-m> -3.78855</x1-m>
      <y1-m> 0.0 </y1-m>
      <z1-m> -0.26532 </z1-m>
      <x2-m> -3.72861 </x2-m>
      <y2-m> 0.0 </y2-m>
      <z2-m> 0.03016 </z2-m>
    </axis>
  </animation>
<animation>
    <type>translate</type>
    <object-name>wheelF</object-name>
    <property>gear/gear/compression-norm</property>
    <factor>.1</factor>
    <axis>
      <x1-m> -3.78855</x1-m>
      <y1-m> 0.0 </y1-m>
      <z1-m> -0.26532 </z1-m>
      <x2-m> -3.72861 </x2-m>
      <y2-m> 0.0 </y2-m>
      <z2-m> 0.03016 </z2-m>
    </axis>
  </animation>
<animation>
    <type>translate</type>
    <object-name>wheelL</object-name>
    <property>gear/gear[1]/compression-norm</property>
    <factor>.1</factor>
    <axis>
      <x1-m> -0.1491</x1-m>
      <y1-m> -0.58916</y1-m>
      <z1-m> -0.29855 </z1-m>
      <x2-m> -0.1491 </x2-m>
      <y2-m> -0.38674 </y2-m>
      <z2-m> 0.0467 </z2-m>
    </axis>
  </animation>
<animation>
    <type>translate</type>
    <object-name>wheelR</object-name>
    <property>gear/gear[2]/compression-norm</property>
    <factor>.1</factor>
    <axis>
      <x1-m> -0.1491</x1-m>
      <y1-m> 0.58916</y1-m>
      <z1-m> -0.29855 </z1-m>
      <x2-m> -0.1491 </x2-m>
      <y2-m>0.38674 </y2-m>
      <z2-m> 0.0467 </z2-m>
    </axis>
  </animation>
